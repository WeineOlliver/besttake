{
    "editor.rulers": [100],
    "editor.tabCompletion": "on",
    "editor.tabSize": 4,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "files.exclude": {
      "**/.DS_Store": true,
      "**/.git": true,
      "**/.hg": true,
      "**/.svn": true,
      "**/CVS": true,
      "dist": true
    }
  }